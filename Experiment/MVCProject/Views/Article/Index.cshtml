@model IEnumerable<ArticleDto>

@{
    ViewData["Title"] = "View";
}

<h1>Articles</h1>

<a href="~/article/create">Create article</a>

@if(Model.Any()){
    @foreach (var item in Model)
    {
        @await Html.PartialAsync("ArticlePreview", item);
    }
}

@section CssStyles
{
    <style>
        h1{
            color: red;
        }
    </style>
}

@PaginatorHelper.GeneratePaginator()

